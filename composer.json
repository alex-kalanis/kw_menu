{
    "name": "alex-kalanis/kw_menu",
    "description": "Menu manager and processor in KWCMS",
    "keywords": [
        "menu",
        "files",
        "tree"
    ],
    "license": "BSD-3-Clause",
    "type": "library",
    "require": {
        "alex-kalanis/kw_paths": ">=3.0",
        "ext-mbstring": "*",
        "php": ">=7.2.0"
    },
    "require-dev": {
        "alex-kalanis/kw_storage": ">=1.0",
        "alex-kalanis/kw_tree": ">=1.0",
        "phpstan/phpstan": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "friendsofphp/php-cs-fixer": "^3.0",
        "psr/container": "=2.0.2",
        "phpunit/phpunit": ">=7.2 <=9",
        "php": ">=7.4.0"
    },
    "suggest": {
        "alex-kalanis/kw_storage": "Local storage for accessing resources",
        "alex-kalanis/kw_tree": "Processing tree for accessing resources"
    },
    "autoload": {
        "psr-4": {
            "kalanis\\kw_menu\\": "php-src/"
        }
    },
    "scripts": {
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
